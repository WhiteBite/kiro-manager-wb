# IMAP Configuration for catch-all email
IMAP_SERVER=
IMAP_USER=
IMAP_PASSWORD=
IMAP_PORT=993

# Email strategy
EMAIL_STRATEGY=single

# Optional: Custom paths
# TOKENS_PATH=C:\Users\Username\.kiro-manager-wb\tokens

# Optional: Debug settings
# DEBUG_VERBOSE=true
# DEBUG_SCREENSHOTS=true

# Optional: Registration settings
# AUTOREG_HEADLESS=false
# AUTOREG_SPOOFING=true

# Optional: Kiro paths (auto-detected if not set)
# KIRO_PATH=C:\Users\Username\AppData\Local\Programs\Kiro

# Optional: Custom Machine ID (64-char hex string)
# CUSTOM_MACHINE_ID=1234567890abcdef1234567890abcdef1234567890abcdef1234567890abcdef
